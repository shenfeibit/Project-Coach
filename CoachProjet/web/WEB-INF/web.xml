    </servlet>
    <servlet-mapping>
        <servlet-name>ServletShowInfoCli</servlet-name>
        <url-pattern>/ServletShowInfoCli</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletProgrammeSt</servlet-name>
        <servlet-class>CtrlServlet.ServletProgrammeSt</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletProgrammeSt</servlet-name>
        <url-pattern>/ServletProgrammeSt</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ServletAffecter</servlet-name>
        <url-pattern>/ServletAffecter</url-pattern>
    </servlet-mapping>

    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
