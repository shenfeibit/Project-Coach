    </servlet>
    <servlet-mapping>
        <servlet-name>ServletClientNonPgrm</servlet-name>
        <url-pattern>/ServletClientNonPgrm</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletObjectifs</servlet-name>
        <servlet-class>CtrlServlet.ServletObjectifs</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletObjectifs</servlet-name>
        <url-pattern>/ServletObjectifs</url-pattern>
